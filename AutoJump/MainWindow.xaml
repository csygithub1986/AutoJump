<Window x:Class="AutoJump.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoJump" Closed="Window_Closed"
        mc:Ignorable="d"
        Title="MainWindow" Height="420" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="239*"/>
            <ColumnDefinition Width="278*"/>
        </Grid.ColumnDefinitions>
        <Image Name="imageBox" Height="384" Width="216"/>
        <Button Click="BtnStart_Click" Name="btnStart" Content="开始" Margin="40,60,0,0" Grid.Column="1" HorizontalAlignment="Left" Width="200" Height="50" VerticalAlignment="Top"/>
        <Button Click="BtnStop_Click" Name="btnStop" Content="停止" IsEnabled="False" Margin="40,157,0,0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="50"/>
    </Grid>
</Window>
